<BorderPane prefHeight="800.0" prefWidth="360.0" styleClass="background" stylesheets="@../css/background.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StatisticsP3Controller">
   <top>
      <GridPane fx:id="mainViewBody" alignment="CENTER" hgap="10.0" prefHeight="65.0" prefWidth="360.0" vgap="10.0" BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints fillWidth="false" halignment="CENTER" hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="72.00000610351563" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Label fx:id="lblDay" alignment="CENTER" contentDisplay="CENTER" prefHeight="55.0" prefWidth="230.0" text="Statistics" textAlignment="CENTER" textFill="#f8f8f8" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
               <effect>
                  <Shadow height="6.34" radius="1.085" width="0.0">
                     <color>
                        <Color red="1.0" green="1.0" blue="1.0" />
                     </color>
                  </Shadow>
               </effect>
               <GridPane.margin>
                  <Insets top="4.0" />
               </GridPane.margin>
            </Label>
         </children>
      </GridPane>
   </top>
   <bottom>
      <FlowPane alignment="CENTER" prefHeight="111.0" prefWidth="360.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="p1Btn" mnemonicParsing="false" onAction="#switchPage1" text="P1" />
            <Button fx:id="p2Btn" mnemonicParsing="false" onAction="#switchPage2" text="P2" />
            <Button fx:id="p3Btn" mnemonicParsing="false" onAction="#switchPage3" text="P3" />
         </children>
      </FlowPane>
   </bottom>
</BorderPane>
